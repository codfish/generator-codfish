{
  "name": "generator-codfish",
  "version": "0.0.0",
  "description": "Yeoman generators to scaffold out personal projects.",
  "homepage": "https://github.com/codfish/generator-codfish",
  "author": {
    "name": "Chris O'Donnell",
    "email": "dev@codfish.io",
    "url": "https://codfish.io/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "repository": "codfish/generator-codfish",
  "license": "MIT",
  "keywords": [
    "yeoman-generator",
    "react",
    "linting",
    "github templates",
    "express api"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "cod-scripts": "^1.0.0",
    "jest": "^24.9.0",
    "markdownlint-cli": "^0.20.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^2.0.0"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6",
    "yarn": ">=1"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "generator-license": "^5.4.0",
    "github-username": "^5.0.1",
    "inquirer": "^7.0.0",
    "inquirer-npm-name": "^3.0.0",
    "lodash": "^4.17.15",
    "yeoman-generator": "^4.0.2"
  },
  "scripts": {
    "format": "cod-scripts format",
    "lint": "cod-scripts lint",
    "lint:md": "markdownlint -i node_modules -i dist -i **/templates/*.md .",
    "validate": "cod-scripts validate"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/cod-scripts/eslint.js"
    ],
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": "off"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "cod-scripts pre-commit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
